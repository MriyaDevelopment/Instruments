<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.google.android.material.button.MaterialButton
        android:id="@+id/start"
        style="@style/OnlyTextButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_marginVertical="@dimen/margin10"
        android:layout_marginEnd="@dimen/margin16"
        android:paddingHorizontal="@dimen/padding14"
        android:text="@string/testCategoriesStart"
        android:textColor="@color/blue_5B67CA"
        android:textSize="@dimen/size16"
        android:textStyle="normal"
        app:cornerRadius="@dimen/size10"
        app:icon="@drawable/ic_arrow_begin"
        app:iconGravity="end"
        app:iconTint="@color/blue_5B67CA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="@color/blue_5B67CA" />

</androidx.constraintlayout.widget.ConstraintLayout>